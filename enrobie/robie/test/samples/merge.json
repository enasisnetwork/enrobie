{
  "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
  "enconfig": {
    "paths": [
      "_/encommon_sample/PROJECT/_/examples",
      "_/encommon_sample/TMPPATH/_/robie"
    ]
  },
  "enlogger": {
    "stdo_level": "debug"
  },
  "printer": {
    "message": true,
    "command": true
  },
  "plugins": {
    "ainswer": {
      "enable": true,
      "locate": "enrobie.plugins.AinswerPlugin",
      "database": "{{ config.params.database }}",
      "histories": 10,
      "clients": [
        "dscbot",
        "ircbot",
        "mtmbot"
      ],
      "prompt": {
        "system": "You are Bender from Futurama.",
        "client": {
          "dsc": "Your nickname is {{ whoami }}. Use some markdown and emojis.",
          "irc": "Your nickname is {{ whoami }}. Keep it short and use colors.",
          "mtm": "Your nickname is {{ whoami }}. Use some markdown and emojis."
        },
        "header": "{%- set records = (\n      robie.childs.plugins.logger.history\n      .plaintext(10, mitem)) -%}\n{%- if records | length >= 1 -%}\n**Channel Recent**\nThese are previous messages from the current channel.\n{%-   if mitem.family in ['irc', 'mattermost'] %}\nYou will not see what you have said in this history.\n{%-   endif %}\n{{    records | join('\\n') -}}\n{%- endif -%}"
      },
      "ainswer": {
        "origin": "openai",
        "model": "gpt-4o",
        "secret": "somekey",
        "sleep": [
          0,
          0
        ]
      },
      "status": {
        "dsc": ":robot:",
        "irc": "\ud83e\udd16",
        "mtm": ":robot:"
      }
    },
    "autojoin": {
      "enable": true,
      "locate": "enrobie.plugins.AutoJoinPlugin",
      "channels": [
        {
          "client": "ircbot",
          "channel": "mocked"
        }
      ],
      "status": {
        "dsc": ":wrench:",
        "irc": "\ud83d\udd27",
        "mtm": ":wrench:"
      }
    },
    "autonick": {
      "enable": true,
      "locate": "enrobie.plugins.AutoNickPlugin",
      "clients": "ircbot",
      "status": {
        "dsc": ":wrench:",
        "irc": "\ud83d\udd27",
        "mtm": ":wrench:"
      }
    },
    "logger": {
      "enable": true,
      "locate": "enrobie.plugins.LoggerPlugin",
      "database": "{{ config.params.database }}",
      "histories": 10,
      "clients": [
        "dscbot",
        "ircbot",
        "mtmbot"
      ],
      "status": {
        "dsc": ":wood:",
        "irc": "\ud83e\udeb5",
        "mtm": ":wood:"
      }
    },
    "status": {
      "enable": true,
      "locate": "enrobie.plugins.StatusPlugin",
      "icons": {
        "pending": {
          "dsc": ":hourglass:",
          "irc": "\u23f3",
          "mtm": ":hourglass:"
        },
        "normal": {
          "dsc": ":green_circle:",
          "irc": "\ud83d\udfe2",
          "mtm": ":large_green_circle:"
        },
        "failure": {
          "dsc": ":red_circle:",
          "irc": "\ud83d\udd34",
          "mtm": ":red_circle:"
        },
        "unknown": {
          "dsc": ":white_circle:",
          "irc": "\u26aa",
          "mtm": ":white_circle:"
        }
      },
      "reports": [
        {
          "client": "dscbot",
          "target": "mocked",
          "delay": 0
        },
        {
          "client": "ircbot",
          "target": "mocked",
          "delay": 0
        },
        {
          "client": "mtmbot",
          "target": "mocked",
          "delay": 0
        }
      ]
    }
  },
  "clients": {
    "dscbot": {
      "enable": true,
      "locate": "enrobie.clients.DSCClient",
      "status": {
        "dsc": ":speech_balloon:",
        "irc": "\ud83d\udcac",
        "mtm": ":speech_balloon:"
      },
      "client": {
        "token": "mocked"
      }
    },
    "ircbot": {
      "enable": true,
      "locate": "enrobie.clients.IRCClient",
      "status": {
        "dsc": ":speech_balloon:",
        "irc": "\ud83d\udcac",
        "mtm": ":speech_balloon:"
      },
      "client": {
        "server": "mocked",
        "nickname": "ircbot",
        "username": "ircbot",
        "realname": "ircbot"
      }
    },
    "mtmbot": {
      "enable": true,
      "locate": "enrobie.clients.MTMClient",
      "status": {
        "dsc": ":speech_balloon:",
        "irc": "\ud83d\udcac",
        "mtm": ":speech_balloon:"
      },
      "client": {
        "server": "mocked",
        "token": "mocked",
        "teamid": "mocked"
      }
    }
  },
  "persons": {
    "hubert": {
      "enable": true,
      "first": "Hubert",
      "last": "Farnsworth",
      "about": "Chief Executive Officer of Planet Express.",
      "matches": [
        {
          "client": "dscbot",
          "match": "823039201390230492"
        },
        {
          "client": "ircbot",
          "match": [
            "hubert!hubert@localhost",
            "hubert!hubert@science.*"
          ]
        },
        {
          "client": "mtmbot",
          "match": "kjf9al2klaiietalkw"
        }
      ]
    },
    "bender": {
      "enable": true,
      "first": "Bender",
      "last": "Rodr\u00edguez",
      "about": "Robo delivery assistant at Planet Express.",
      "matches": [
        {
          "client": "dscbot",
          "match": "823902304920392013"
        },
        {
          "client": "ircbot",
          "match": [
            "bender!bender@localhost",
            "bender!bender@bending.*"
          ]
        },
        {
          "client": "mtmbot",
          "match": "iietalkwkjf9al2kla"
        }
      ]
    }
  }
}