{
  "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
  "enconfig": {
    "paths": [
      "_/encommon_sample/PROJECT/_/examples",
      "_/encommon_sample/TMPPATH/_/robie"
    ]
  },
  "enlogger": {
    "stdo_level": "debug"
  },
  "printer": {
    "message": true,
    "command": true
  },
  "clients": {
    "dscbot": {
      "enable": true,
      "status": {
        "dsc": "<:discord:mocked>",
        "irc": "\ud83d\udcac",
        "mtm": ":discord:"
      },
      "client": {
        "token": "mocked"
      }
    },
    "ircbot": {
      "enable": true,
      "status": {
        "dsc": "<:irc:mocked>",
        "irc": "\ud83d\udcac",
        "mtm": ":irc:"
      },
      "client": {
        "server": "mocked",
        "nickname": "ircbot",
        "username": "ircbot",
        "realname": "ircbot"
      }
    },
    "mtmbot": {
      "enable": true,
      "status": {
        "dsc": "<:mattermost:mocked>",
        "irc": "\ud83d\udcac",
        "mtm": ":mattermost:"
      },
      "client": {
        "server": "mocked",
        "token": "mocked",
        "teamid": "mocked"
      }
    }
  },
  "plugins": {
    "status": {
      "enable": true,
      "icons": {
        "pending": {
          "dsc": "<:initial:mocked>",
          "irc": "\u23f3",
          "mtm": ":initial:"
        },
        "normal": {
          "dsc": "<:success:mocked>",
          "irc": "\ud83d\udfe2",
          "mtm": ":success:"
        },
        "failure": {
          "dsc": "<:failure:mocked>",
          "irc": "\ud83d\udd34",
          "mtm": ":failure:"
        },
        "unknown": {
          "dsc": "<:unknown:mocked>",
          "irc": "\u26aa",
          "mtm": ":unknown:"
        }
      },
      "reports": [
        {
          "client": "dscbot",
          "target": "mocked",
          "delay": 0
        },
        {
          "client": "ircbot",
          "target": "mocked",
          "delay": 0
        },
        {
          "client": "mtmbot",
          "target": "mocked",
          "delay": 0
        }
      ]
    },
    "autojoin": {
      "enable": true,
      "channels": [
        {
          "client": "ircbot",
          "channel": "mocked"
        }
      ],
      "status": {
        "dsc": "<:unknown:mocked>",
        "irc": "\u2699",
        "mtm": ":unknown:"
      }
    },
    "autonick": {
      "enable": true,
      "clients": "ircbot",
      "status": {
        "dsc": "<:unknown:mocked>",
        "irc": "\u2699",
        "mtm": ":unknown:"
      }
    },
    "ainswer": {
      "enable": true,
      "database": "{{ config.params.database }}",
      "clients": [
        "dscbot",
        "ircbot",
        "mtmbot"
      ],
      "prompt": {
        "system": "You are Bender from Futurama.",
        "client": {
          "dsc": "Your nickname is {{ whoami }}. Use some markdown and emojis.",
          "irc": "Your nickname is {{ whoami }}. Keep it short and use colors.",
          "mtm": "Your nickname is {{ whoami }}. Use some markdown and emojis."
        }
      },
      "ainswer": {
        "origin": "openai",
        "model": "gpt-4o",
        "secret": "somekey"
      },
      "status": {
        "dsc": "<:unknown:mocked>",
        "irc": "\ud83d\udcac",
        "mtm": ":unknown:"
      }
    }
  }
}