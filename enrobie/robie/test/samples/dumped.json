{
  "enconfig": {
    "paths": [
      "_/encommon_sample/PROJECT/_/examples",
      "_/encommon_sample/TMPPATH/_/robie"
    ]
  },
  "enlogger": {
    "stdo_level": "debug",
    "file_level": null,
    "file_path": null
  },
  "encrypts": null,
  "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
  "printer": {
    "message": true,
    "command": true
  },
  "service": {
    "respite": {
      "health": 3
    }
  },
  "clients": {
    "dscbot": {
      "name": "dscbot",
      "kind": "client",
      "params": {
        "enable": true,
        "locate": "enrobie.clients.DSCClient",
        "client": {
          "token": "mocked",
          "timeout": 30,
          "ssl_verify": true,
          "ssl_capem": null,
          "queue_size": 10000
        },
        "intents": 4609,
        "delay": 15,
        "status": {
          "irc": "\ud83d\udcac",
          "dsc": ":speech_balloon:",
          "mtm": ":speech_balloon:"
        }
      },
      "family": "discord"
    },
    "ircbot": {
      "name": "ircbot",
      "kind": "client",
      "params": {
        "enable": true,
        "locate": "enrobie.clients.IRCClient",
        "client": {
          "server": "mocked",
          "port": 6697,
          "timeout": 30,
          "operate": "normal",
          "nickname": "ircbot",
          "username": "ircbot",
          "realname": "ircbot",
          "password": null,
          "servername": "services.invalid",
          "serverid": "42X",
          "ssl_enable": true,
          "ssl_verify": true,
          "queue_size": 10000
        },
        "delay": 15,
        "status": {
          "irc": "\ud83d\udcac",
          "dsc": ":speech_balloon:",
          "mtm": ":speech_balloon:"
        }
      },
      "family": "irc"
    },
    "mtmbot": {
      "name": "mtmbot",
      "kind": "client",
      "params": {
        "enable": true,
        "locate": "enrobie.clients.MTMClient",
        "client": {
          "server": "mocked",
          "port": 443,
          "token": "mocked",
          "teamid": "mocked",
          "timeout": 30,
          "ssl_verify": true,
          "ssl_capem": null,
          "queue_size": 10000
        },
        "delay": 15,
        "status": {
          "irc": "\ud83d\udcac",
          "dsc": ":speech_balloon:",
          "mtm": ":speech_balloon:"
        }
      },
      "family": "mattermost"
    }
  },
  "plugins": {
    "ainswer": {
      "name": "ainswer",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.AinswerPlugin",
        "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
        "histories": 10,
        "clients": [
          "dscbot",
          "ircbot",
          "mtmbot"
        ],
        "ainswer": {
          "origin": "openai",
          "model": "gpt-4o",
          "secret": "somekey",
          "timeout": 30,
          "sleep": [
            0,
            0
          ]
        },
        "prompt": {
          "system": "You are Bender from Futurama.",
          "client": {
            "dsc": "Your nickname is {{ whoami }}. Use some markdown and emojis.",
            "irc": "Your nickname is {{ whoami }}. Keep it short and use colors.",
            "mtm": "Your nickname is {{ whoami }}. Use some markdown and emojis."
          },
          "header": null,
          "footer": null,
          "ignore": [
            "If you believe that you are being abused by the user asking the quesiton."
          ]
        },
        "logger": "logger",
        "status": {
          "irc": "\ud83e\udd16",
          "dsc": ":robot:",
          "mtm": ":robot:"
        }
      }
    },
    "autojoin": {
      "name": "autojoin",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.AutoJoinPlugin",
        "channels": [
          {
            "client": "ircbot",
            "channel": "mocked"
          }
        ],
        "interval": 5,
        "status": {
          "irc": "\ud83d\udd27",
          "dsc": ":wrench:",
          "mtm": ":wrench:"
        }
      }
    },
    "autonick": {
      "name": "autonick",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.AutoNickPlugin",
        "clients": [
          "ircbot"
        ],
        "interval": 5,
        "status": {
          "irc": "\ud83d\udd27",
          "dsc": ":wrench:",
          "mtm": ":wrench:"
        }
      }
    },
    "enhomie": {
      "name": "enhomie",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.HomiePlugin",
        "command": {
          "irc": "!persist",
          "dsc": "!persist",
          "mtm": "!persist"
        },
        "restful": "http://mocked:8420",
        "username": "username",
        "password": "password",
        "timeout": 30,
        "ssl_verify": true,
        "ssl_capem": null,
        "clients": [
          "dscbot",
          "ircbot",
          "mtmbot"
        ],
        "status": {
          "irc": "\ud83d\udca1",
          "dsc": ":bulb:",
          "mtm": ":bulb:"
        }
      }
    },
    "logger": {
      "name": "logger",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.LoggerPlugin",
        "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
        "histories": 10,
        "clients": [
          "dscbot",
          "ircbot",
          "mtmbot"
        ],
        "output": null,
        "status": {
          "irc": "\ud83e\udeb5",
          "dsc": ":wood:",
          "mtm": ":wood:"
        }
      }
    },
    "nagios": {
      "name": "nagios",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.NagiosPlugin",
        "command": {
          "irc": "!nagios",
          "dsc": "!nagios",
          "mtm": "!nagios"
        },
        "restful": "http://mocked/nagios",
        "username": "username",
        "password": "password",
        "timeout": 30,
        "ssl_verify": true,
        "ssl_capem": null,
        "clients": [
          "dscbot",
          "ircbot",
          "mtmbot"
        ],
        "status": {
          "irc": "\ud83d\udd0d",
          "dsc": ":mag:",
          "mtm": ":mag:"
        }
      }
    },
    "status": {
      "name": "status",
      "kind": "plugin",
      "params": {
        "enable": true,
        "locate": "enrobie.plugins.StatusPlugin",
        "reports": [
          {
            "client": "dscbot",
            "target": "mocked",
            "states": null,
            "delay": 0
          },
          {
            "client": "ircbot",
            "target": "mocked",
            "states": null,
            "delay": 0
          },
          {
            "client": "mtmbot",
            "target": "mocked",
            "states": null,
            "delay": 0
          }
        ],
        "command": {
          "irc": "!status",
          "dsc": "!status",
          "mtm": "!status"
        },
        "icons": {
          "pending": {
            "irc": "\u23f3",
            "dsc": ":hourglass:",
            "mtm": ":hourglass:"
          },
          "normal": {
            "irc": "\ud83d\udfe2",
            "dsc": ":green_circle:",
            "mtm": ":large_green_circle:"
          },
          "failure": {
            "irc": "\ud83d\udd34",
            "dsc": ":red_circle:",
            "mtm": ":red_circle:"
          },
          "unknown": {
            "irc": "\u26aa",
            "dsc": ":white_circle:",
            "mtm": ":white_circle:"
          }
        }
      }
    }
  },
  "persons": {
